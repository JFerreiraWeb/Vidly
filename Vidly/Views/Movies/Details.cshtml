@model Vidly.Models.Movie
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<ul>
    <li><b>Genre:</b> @Model.GenreType.Name</li>
    <li><b>Date Added:</b> @Model.DateAdded.ToShortDateString()</li>
    <li><b>Date Released:</b> @Model.DateOFRelease.ToShortDateString()</li>
    <li><b>Number in Stock:</b> @Model.NumberInStock</li>

</ul>


